<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.first.springProject.mapper.TravelMapper">

	<select id="getTravelList"
		resultType="com.first.springProject.domain.Travel">
		select * from travel;
	</select>
	
	<select id = "getTravel"
		resultType="com.first.springProject.domain.Travel">
		select * from travel
		where trv_no = #{trv_no}
		;
	</select>
	<insert id="registTravel"
		parameterType="com.first.springProject.domain.Travel">
		insert into travel (trv_no, title)
		values (#{trv_no}, #{title])		
	</insert>
	
</mapper>